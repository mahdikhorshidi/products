{
    "name": "mahdikhorshidi/products",
    "type": "library",
    "description": "Products arenge",
    "keywords": [
        "mahdikhorshidi",
        "products"
    ],
    "homepage": "https://github.com/mahdikhorshidi/products",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahdi Khorshidi",
            "email": "mahdikhorshidi@gmail.com",
            "homepage": "http://mahdikhorshidi.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3",
        "bhavinjr/laravel-wishlist": "^1.0",
        "cybercog/laravel-likeable": "^3.0",
        "spatie/laravel-sluggable": "^2.1",
        "mahdikhorshidi/categories": "dev-master",
        "spatie/laravel-tags": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "mahdikhorshidi\\products\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mahdikhorshidi\\products\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
